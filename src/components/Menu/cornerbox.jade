doctype html
html.no-js(lang='en')
  head
    meta(charset='UTF-8')
    |
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    |
    |
    meta(name='viewport', content='width=device-width, initial-scale=1')
    |
    |
    title Off-Canvas menu-wrap Effects - Corner Box
    |
    meta(name='description', content='Modern effects and styles for off-canvas navigation with CSS transitions and SVG animations using Snap.svg')
    |
    meta(name='keywords', content='sidebar, off-canvas, menu-wrap, navigation, effect, inspiration, css transition, SVG, morphing, animation')
    |
    meta(name='author', content='Codrops')
    |
    link(rel='shortcut icon', href='../favicon.ico')
    |
    link(rel='stylesheet', type='text/css', href='css/normalize.css')
    |
    link(rel='stylesheet', type='text/css', href='css/demo.css')
    |
    link(rel='stylesheet', type='text/css', href='fonts/font-awesome-4.2.0/css/font-awesome.min.css')
    |
    link(rel='stylesheet', type='text/css', href='css/menu_cornerbox.css')
    //if IE
      script(src='http://html5shiv.googlecode.com/svn/trunk/html5.js')
  |
  body
    .content-wrap
      .menu-wrap
        nav.menu-wrap
          .profile
            img(src='img/user1.png', alt='Matthew Greenberg')
            span Matthew Greenberg
          |
          .icon-list
            a(href='#')
              i.fa.fa-fw.fa-star-o
              span Favorites
            |
            a(href='#')
              i.fa.fa-fw.fa-bell-o
              span Alerts
            |
            a(href='#')
              i.fa.fa-fw.fa-envelope-o
              span Messages
            |
            a(href='#')
              i.fa.fa-fw.fa-comment-o
              span Comments
        |
        button#close-button.close-button Close menu-wrap
      |
      button#open-button.menu-button Open menu-wrap
      |
      .content-wrap
        .content
          header.codrops-header
            .codrops-links
              a.codrops-icon.codrops-icon-prev(href='http://tympanus.net/Development/TabStylesInspiration/')
                span Previous Demo
              |
              a.codrops-icon.codrops-icon-drop(href='http://tympanus.net/codrops/?p=20100')
                span Back to the Codrops Article
            |
            h1
              | Off-Canvas menu-wrap Effects
              span Showing (off-canvas) menus stylishly
            |
            nav.codrops-demos
              a(href='index.html') Top Side
              |
              a(href='sideslide.html') Side Slide
              |
              a.current-demo(href='cornerbox.html') Corner Box
              |
              a(href='cornerbox_nested.html') Nested Corner Box
              |
              a(href='topexpand.html') Top Expand
              |
              a(href='cornermorph.html') Corner Morph
              |
              a(href='elastic.html') Elastic
              |
              a(href='bubble.html') Bubble
              |
              a(href='wave.html') Wave
          // Related demos
          section.related
            p If you enjoyed this demo you might also like:
            |
            a(href='http://tympanus.net/Development/SidebarTransitions/')
              img(src='img/related/sidebartransitions.png')
              |
              h3 Transitions for Off-Canvas Navigations
            |
            a(href='http://tympanus.net/Development/PerspectivePageViewNavigation/')
              img(src='img/related/PerspectiveNavigation.png')
              |
              h3 Perspective Page View Navigation
      // /content-wrap
    // /content-wrap
    script(src='js/classie.js')
    |
    script(src='js/content.js')
